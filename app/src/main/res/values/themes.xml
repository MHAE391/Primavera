<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Primavera" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/topBack</item>
        <item name="colorPrimaryDark">@color/topBack</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.Primavera.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="fab_square" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">5dp</item>
    </style>

</resources>